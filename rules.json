{
    "rules": {
        "file": [{
            "match": "(.*)S[0-9]{2}E[0-9]{2}(.*)(mkv|mp4|avi)$",
            "command": "echo {0}"
        },{
            "match": "(.*)[0-9]{4}(-|\\.)[0-9]{2}(-|\\.)[0-9]{2}(.*)(mkv|mp4|avi)$",
            "command": "echo {0}"
        }],
        "folder": []
    },
    "ignore": {
        "file": [
            "(.*)(nfo|srt)$",
            "(.*)r[0-9]{2}$"
        ],
        "folder": [
            "(.*)WhatSeeds$"
        ]
    }
}
